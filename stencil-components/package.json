{
  "name": "stencil-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "start:stencil": "stencil build --dev --watch --docs",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "start:storybook": "npm-run-all --parallel start:stencil storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s www"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@stencil/core": "^1.3.3",
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-notes": "^5.2.0",
    "@storybook/addon-viewport": "^5.2.0",
    "@storybook/html": "^5.2.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT"
}
